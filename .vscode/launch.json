{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "MkDocs",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/.venv/bin/mkdocs",
      "args": [
        "serve"
      ],
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/.venv/bin/python"
      }
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/asihan/repos/leetpattern/src/cpp",
      "program": "/home/asihan/repos/leetpattern/src/cpp/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
